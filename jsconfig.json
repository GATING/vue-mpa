{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "baseUrl": "./",
    "moduleResolution": "node",
    "paths": {
      "@/*": ["src/*"],
      "@api/*": ["./src/api/*"],
      "@util/*": ["./src/utils/*"],
      "@view/*": ["./src/views/*"],
      "@comp/*": ["./src/components/*"],
      "@style/*": ["./src/styles/*"],
      "@plugin/*": ["./src/plugins/*"]
    },
    "lib": ["esnext", "dom", "dom.iterable", "scripthost"]
  },
  "exclude": ["node_modules", "dist"]
}
